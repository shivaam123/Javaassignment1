	Program to iterate through all elements in a hash list
  
   package com.hashset;

import java.util.HashSet;

public class appendhash {

	public static void main(String[] args) {
	     HashSet<String> h_set = new HashSet<String>();
	          h_set.add("Messi");
	          h_set.add("Ronaldo");
	          h_set.add("Kaka");
	          h_set.add("Neymar");
	          h_set.add("Luka");

	   // print the hash set
	   System.out.println("The Hash Set: " + h_set);

	}
  
